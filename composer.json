{
	"name" : "coka/api-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka API Bundle",
	"homepage" : "https://github.com/CedrickOka/oka-api",
	"license" : "MIT",
	"keywords" : [
		"rest",
		"api",
		"security",
		"WSSE",
		"CORS"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"prefer-stable" : true,
	"minimum-stability" : "dev",
	"require" : {
		"php" : "^5.5 || >=7.0",
		"symfony/symfony" : "^3.4 || ^4.0"
	},
	"require-dev" : {
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/orm" : "^2.5",
		"phpunit/phpunit" : "~4.8|~5.0",
		"symfony/phpunit-bridge" : "^3.0"
	},
	"suggests" : {
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/orm" : "^2.5",
		"jms/serializer-bundle" : "^2.3"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\ApiBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\ApiBundle\\Tests\\" : "tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "3.x.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/oka-api/blob/master/src/Resources/doc/index.md"
	}
}


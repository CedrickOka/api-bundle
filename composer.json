{
	"name" : "coka/api-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka API Bundle",
	"homepage" : "https://github.com/CedrickOka/oka-api",
	"license" : "MIT",
	"keywords" : [
		"rest",
		"api",
		"security",
		"JSON Web Token",
		"WSSE",
		"CORS"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
		"php" : "^5.3.9 || ^7.0",
		"symfony/symfony" : "2.8.*",
		"doctrine/orm" : "^2.4.8",
		"doctrine/doctrine-bundle" : "~1.4",
		"symfony/framework-bundle" : "^2.7 || ^3.0",
		"symfony/security-bundle" : "^2.7 || ^3.0"
	},
	"require-dev" : {
		"friendsofphp/php-cs-fixer" : "^1.11",
		"swiftmailer/swiftmailer" : "^4.3 || ^5.0",
		"symfony/console" : "^2.7 || ^3.0",
		"symfony/phpunit-bridge" : "^2.7 || ^3.0",
		"symfony/validator" : "^2.7 || ^3.0",
		"symfony/yaml" : "^2.7 || ^3.0",
		"jms/serializer-bundle" : "@stable",
		"phpunit/phpunit" : "~4.8|~5.0",
		"lcobucci/jwt" : "^3.2"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\ApiBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\ApiBundle\\Tests\\" : "Tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x.x-dev"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/oka-api/blob/master/Resources/doc/index.md"
	}
}
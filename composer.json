{
	"name" : "coka/api-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka API Bundle",
	"homepage" : "https://github.com/CedrickOka/oka-api",
	"license" : "MIT",
	"keywords" : [
		"rest",
		"api",
		"security",
		"JSON Web Token",
		"WSSE",
		"CORS"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
		"php" : "^5.5 || >=7.0",
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/orm" : "^2.5",
		"incenteev/composer-parameter-handler" : "^2.0",
		"jms/serializer-bundle" : "^2.3",
		"knplabs/doctrine-behaviors" : "^1.5",
		"ramsey/uuid" : "^3.6",
		"symfony/symfony" : "^2.8 || ^3.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.8|~5.0",
		"symfony/phpunit-bridge" : "^3.0"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\ApiBundle\\" : "src"
		},
		"exclude-from-classmap" : [
			"/src/Tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\ApiBundle\\Tests\\" : "src/Tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.x.x-dev"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/oka-api/blob/master/Resources/doc/index.md"
	}
}


{
	"name" : "coka/api-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka API Bundle",
	"homepage" : "https://github.com/CedrickOka/oka-api",
	"license" : "MIT",
	"keywords" : [
		"rest",
		"api",
		"security",
		"WSSE",
		"CORS"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\ApiBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\ApiBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"doctrine/annotations" : "^1.2",
		"symfony/serializer" : "^3.4 || ^4.0",
		"symfony/security-bundle" : "^3.4 || ^4.0",
		"symfony/monolog-bundle" : "^3 || ^4.0",
		"symfony/framework-bundle" : "^3.4 || ^4.0",
		"symfony/event-dispatcher" : "^3.4 || ^4.0",
		"symfony/dependency-injection" : "^3.4 || ^4.0",
		"symfony/console" : "^3.4 || ^4.0",
		"symfony/config" : "^3.4 || ^4.0",
		"symfony/translation" : "^3.4 || ^4.1",
		"symfony/validator" : "^3.4 || ^4.1",
		"symfony/stopwatch" : "^3.4 || ^4.1"
	},
	"require-dev" : {
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/orm" : "^2.5",
		"phpunit/phpunit" : "~4.8|~5.0",
		"symfony/phpunit-bridge" : "^3.0"
	},
	"suggests" : {
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/orm" : "^2.5",
		"jms/serializer-bundle" : "^2.3"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "3.1.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/oka-api/blob/master/src/Resources/doc/index.md"
	}
}

